@model Models.Programador

@{
    ViewData["Title"] = "Detalles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detalles</h1>

<div>
    <h4>Programador @Model.Nombre</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Perfil
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.TipoProgramador.Nombre)
        </dd>
        <dt class="col-sm-2">
            Nombre
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class="col-sm-2">
            Apellidos
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class="col-sm-2">
            DNI O NIE
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DNI_NIE)
        </dd>
        <dt class="col-sm-2">
            Edad
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Edad)
        </dd>
    </dl>
</div>
<div>
    <td class="btn-group">
        <a class="btn btn-outline-primary" asp-action="Edit" asp-route-id="@Model.Id">Editar</a>
        <a class="btn btn-outline-info" asp-action="Index">Volver</a>
    </td>
</div>
