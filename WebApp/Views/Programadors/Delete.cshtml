@model Models.Programador

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="alert alert-danger" role="alert">
    <h4 class="alert-heading">Eliminar</h4>
    <p>wooooowo cuidado , vas a eliminar a @Model.Nombre</p>
    <hr>
    <form  asp-action="Delete">
        
        <td class="btn-group">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Eliminar" class="btn btn-outline-danger" />
            <a class="btn btn-outline-info" asp-action="Index">Volver</a>
        </td>
    </form>
</div>
<div>
    <h4>Programador</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           Tipo de Programador
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoProgramador.Nombre)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Apellido)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Edad)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Edad)
        </dd>
        <dt class = "col-sm-2">
            DNI o NIE
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DNI_NIE)
        </dd>
    </dl>
    

</div>
